{
    "version": 2,
    "builds": [
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/static/test.html"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/static/test.html"
        }
    ],
    "env": {
        "RUST_LOG": "info"
    }
} 